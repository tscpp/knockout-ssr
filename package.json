{
  "name": "knockout-ssr",
  "version": "0.2.0",
  "private": true,
  "description": "Simple server-side rendering for Knockout.js",
  "keywords": [
    "knockout",
    "ssr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tscpp/knockout-ssr.git"
  },
  "license": "MIT",
  "author": "Elias Skogevall",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./bin/knockout-ssr.js": {
      "types": "./bin/knockout-ssr.d.ts",
      "default": "./bin/knockout-ssr.js"
    }
  },
  "types": "index.d.ts",
  "bin": "bin/knockout-ssr.js",
  "scripts": {
    "build": "node ./scripts/build.js",
    "format": "prettier --write .",
    "release": "np",
    "test": "node --test"
  },
  "dependencies": {
    "acorn": "^8.11.3",
    "acorn-walk": "^8.3.1",
    "import-meta-resolve": "^4.0.0",
    "magic-string": "^0.30.5",
    "parse5": "^7.1.2",
    "prettier": "^3.1.1",
    "schema-utils": "^4.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.10.6",
    "@types/yargs": "^17.0.32",
    "execa": "^8.0.1",
    "np": "^9.2.0",
    "source-map-support": "^0.5.21",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "includeDependencies": [
      "@types/node",
      "@types/yargs"
    ]
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  }
}
