{
  "name": "knockout-ssr",
  "version": "0.5.0",
  "description": "Simple server-side rendering for Knockout.js",
  "keywords": [
    "knockout",
    "ssr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tscpp/knockout-ssr.git"
  },
  "license": "MIT",
  "author": "Elias Skogevall",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./bin/knockout-ssr.js": {
      "types": "./bin/knockout-ssr.d.ts",
      "default": "./bin/knockout-ssr.js"
    },
    "./rollup": {
      "types": "./rollup/index.d.ts",
      "default": "./rollup/index.js"
    },
    "./vite": {
      "types": "./vite/index.d.ts",
      "default": "./vite/index.js"
    },
    "./webpack": {
      "types": "./webpack/index.d.ts",
      "default": "./webpack/index.js"
    }
  },
  "types": "index.d.ts",
  "bin": "bin/knockout-ssr.js",
  "scripts": {
    "build": "node ./scripts/build.js",
    "format": "prettier --write .",
    "release": "node scripts/release.js",
    "test": "node test/run-all.js",
    "test:unit": "node test/run-unit.js",
    "test:e2e": "node test/run-e2e.js",
    "type-check": "tsc -b ."
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "acorn": "^8.11.3",
    "import-meta-resolve": "^4.0.0",
    "inline-style-parser": "^0.2.2",
    "loader-utils": "^3.2.1",
    "magic-string": "^0.30.5",
    "parse5": "^7.1.2",
    "prettier": "^3.1.1",
    "schema-utils": "^4.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@swc/core": "^1.3.102",
    "@swc/helpers": "^0.5.3",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/loader-utils": "^2.0.6",
    "@types/node": "^20.10.6",
    "@types/yargs": "^17.0.32",
    "chalk": "^5.3.0",
    "es-module-lexer": "^1.4.1",
    "execa": "^8.0.1",
    "get-port": "^7.0.0",
    "globby": "^14.0.0",
    "memfs": "^4.6.0",
    "np": "^9.2.0",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.14.1",
    "rollup": "^4.9.2",
    "slash": "^5.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^11.0.0-beta.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "knockout": "^3.5.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "includeDependencies": [
      "@types/node",
      "@types/yargs",
      "rollup",
      "vite"
    ]
  },
  "overrides": {
    "ts-node": "$ts-node"
  }
}
